void print(bool x, bool y) {
  cout << "x: ";
  cout << x;
  cout << "\n";

  cout << "y: ";
  cout << y;
  cout << "\n";

  cout << "(x = y;)";
  cout << "\n";

  x = y;

  cout << "x: ";
  cout << x;
  cout << "\n";

  cout << "y: ";
  cout << y;
  cout << "\n";

  cout << "\n";
}
  
void main() {
  bool x;
  bool y;
  while(true) {
    cout << "\n";
    cin >> x;
    cin >> y;
    print(x, y);
  }
}
