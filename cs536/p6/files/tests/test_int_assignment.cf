void print(int x, int y) {
  cout << "x: ";
  cout << x;
  cout << "\n";

  cout << "y: ";
  cout << y;
  cout << "\n";

  cout << "(x = y;)";
  cout << "\n";

  x = y;

  cout << "x: ";
  cout << x;
  cout << "\n";

  cout << "y: ";
  cout << y;
  cout << "\n";

  cout << "\n";
}
  
void main() {
  int x;
  int y;
  while(true) {
    cout << "Give me one int:";
    cout << "\n";
    cin >> x;
    cout << "Give me another:";
    cout << "\n";
    cin >> y;
    print(x, y);
  }
}
