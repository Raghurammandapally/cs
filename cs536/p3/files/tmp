
/* Terminals (tokens returned by the scanner) */
terminal                INT;
terminal                BOOL;
terminal                VOID;
//https://piazza.com/class/id543c6knik6h7?cid=95
terminal TokenVal       TRUE;
terminal TokenVal       FALSE;
